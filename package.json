{
  "name": "nuxt-multi-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "mobile:dev": "nuxt --config-file app/mobile/nuxt.config.js",
    "desktop:dev": "nuxt --config-file app/desktop/nuxt.config.js",
    "dev": "concurrently \"npm run mobile:dev\" \"npm run desktop:dev\"",
    "build:mobile": "nuxt build --config-file app/mobile/nuxt.config.js",
    "build:desktop": "nuxt build --config-file app/desktop/nuxt.config.js",
    "start:mobile": "nuxt start --config-file app/mobile/nuxt.config.js",
    "start:desktop": "nuxt start --config-file app/desktop/nuxt.config.js",
    "lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "prepare": "husky install",
    "lint": "yarn lint:js"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/composition-api": "^0.29.2",
    "@nuxtjs/pwa": "^3.3.5",
    "core-js": "^3.15.1",
    "nuxt": "^2.15.8"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.14.7",
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@nuxt/types": "^2.15.8",
    "@nuxt/typescript-build": "^2.1.0",
    "@nuxtjs/eslint-config-typescript": "^6.0.1",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/tailwindcss": "^4.2.0",
    "concurrently": "^6.3.0",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-vue": "^7.12.1",
    "husky": "^6.0.0",
    "lint-staged": "^10.5.4",
    "postcss": "^8.3.5",
    "prettier": "^2.3.2"
  }
}
